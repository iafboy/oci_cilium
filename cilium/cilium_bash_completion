    flags+=("--k8s-int-traffic-policy=")
    two_word_flags+=("--k8s-int-traffic-policy")
    local_nonpersistent_flags+=("--k8s-int-traffic-policy")
    local_nonpersistent_flags+=("--k8s-int-traffic-policy=")
    flags+=("--k8s-load-balancer")
    local_nonpersistent_flags+=("--k8s-load-balancer")
    flags+=("--k8s-node-port")
    local_nonpersistent_flags+=("--k8s-node-port")
    flags+=("--local-redirect")
    local_nonpersistent_flags+=("--local-redirect")
    flags+=("--states=")
    two_word_flags+=("--states")
    local_nonpersistent_flags+=("--states")
    local_nonpersistent_flags+=("--states=")
    flags+=("--config=")
    two_word_flags+=("--config")
    flags+=("--debug")
    flags+=("-D")
    flags+=("--host=")
    two_word_flags+=("--host")
    two_word_flags+=("-H")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

_cilium_service()
{
    last_command="cilium_service"

    command_aliases=()

    commands=()
    commands+=("delete")
    commands+=("get")
    commands+=("list")
    if [[ -z "${BASH_VERSION:-}" || "${BASH_VERSINFO[0]:-}" -gt 3 ]]; then
        command_aliases+=("ls")
        aliashash["ls"]="list"
    fi
    commands+=("update")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--config=")
    two_word_flags+=("--config")
    flags+=("--debug")
    flags+=("-D")
    flags+=("--host=")
    two_word_flags+=("--host")
    two_word_flags+=("-H")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

